<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.let_server.domain.allergy.mapper.AllergyUserMapper">

    <insert id="insertAllergyUser" parameterType="AllergyUser" useGeneratedKeys="true" keyProperty="allergyUserId">
